generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


model Event {
  id        Int      @id @default(autoincrement())
  title     String
  date      DateTime
  time      DateTime
  type      String
  location  String?
  promotion String?
  image     String?
}
